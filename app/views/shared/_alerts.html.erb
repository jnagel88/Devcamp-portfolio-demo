<% if flash[:alert] %>
	<%= js add_gritter(flash[:alert], title: "Joe Nagel's Portoflio", sticky: false) %>
<% elsif flash[:error] %>
	<%= js add_gritter(flash[:error], title: "Joe Nagel's Portoflio", sticky:false) %>
<% else %>
	<%= js add_gritter(flash[:notice], title: "Joe Nagel's Portoflio", sticky:false) %>
<% end %>
